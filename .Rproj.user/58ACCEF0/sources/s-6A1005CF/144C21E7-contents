# data.frame demo
stu <- c("지훈","유진","동현","민지")
eng <- c(90,80,60,70)
math <- c(50,100,80,30)
df_stu <- data.frame(stu,eng,math)
df_stu
df_stu <- data.frame(stu = c("훈","유진","동현","민지"),
                     eng = c(90,80,60,70),
                     math = c(50,100,80,30)
                     )
df_stu


